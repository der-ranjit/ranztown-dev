{
  "name": "angular-nui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NUI_MODE=dev-server&& ng serve",
    "test": "ng test",
    "nui:build": "ng build --configuration production --source-map=false",
    "nui:watch": "ng build --configuration development --watch --source-map=false",
    "nui:copy-assets": "node building/copy-assets.js",
    "fivem:watch-js": "webpack --config ./config/fivem-webpack.config.js --watch",
    "fivem:build-js": "webpack --config ./config/fivem-webpack.config.js",
    "fivem:watch-lua": "chokidar \"src/fivem-scripts/*.lua\" -c \"npm run fivem:create\"",
    "fivem:create": "node building/create-fivem-resource.js",
    "all:build": "npm run clean && npm run nui:build && npm run fivem:build-js && npm run fivem:watch-lua && npm run fivem:create",
    "all:watch": "npm run clean && concurrently --kill-others \"npm:nui:watch\" \"npm:fivem:watch-js\" \"npm:fivem:watch-lua\"",
    "clean": "rimraf dist/**/*"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/cdk": "^12.1.2",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/material": "^12.1.2",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@citizenfx/client": "^2.0.4207-1",
    "@citizenfx/server": "^2.0.4207-1",
    "@citizenfx/http-wrapper": "^0.2.2",
    "@citizenfx/three": "^0.100.0",
    "fivem-js": "^1.5.2",
    "koa": "^2.6.2",
    "koa-body": "^4.0.6",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "lowdb": "^2.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.1.0",
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@types/fs-extra": "^9.0.12",
    "@types/jasmine": "~3.6.0",
    "@types/koa": "^2.11.6",
    "@types/koa-static": "^4.0.2",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/lodash": "^4.14.171",
    "@types/mv": "^2.1.0",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.1",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "handlebars": "^4.7.7",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mv": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.3",
    "tsc-watch": "^4.4.0",
    "typescript": "~4.3.2",
    "uuid": "^3.3.2",
    "webpack-cli": "^4.7.2",
    "webpack-shell-plugin-next": "^2.2.2"
  }
}
